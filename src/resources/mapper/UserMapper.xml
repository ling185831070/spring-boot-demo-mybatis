<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.ling.springboot.tk.mybatis.mapper.UserMapper">
     <resultMap id="User" type="org.ling.springboot.tk.mybatis.domain.User">
		<id property="unid" column="UNID" />
        <result property="userName" column="USER_NAME" />
        <result property="email" column="EMAIL" />
        <result property="mobile" column="MOBILE" />
        <result property="status" column="STATUS" />
        <result property="validFlag" column="VALID_FLAG" />
        <result property="createdUser" column="CREATED_USER" />
        <result property="createdDate" column="CREATED_DATE" />
        <result property="updatedUser" column="UPDATED_USER" />
        <result property="updatedDate" column="UPDATED_DATE" />
    </resultMap>

    <select id="queryPage"  resultMap="User">
		SELECT * FROM t_user
	</select>
	

</mapper>
